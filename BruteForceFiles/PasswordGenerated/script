import random   
from sre_parse import SPECIAL_CHARS
import string

#max length due to string.ascii plus number plus special characters

ALL_LETTERS = string.ascii_letters
NUMBERS = '0123456789'
SPECIAL_CHARS = '+-*%!$&_'

ALL_SYMBOLS = ALL_LETTERS + NUMBERS + SPECIAL_CHARS

lenght = int(input("lenght of the password (maximum 70 characters): "))

password = ''.join(random.sample(ALL_SYMBOLS, lenght))

print(ALL_LETTERS)
